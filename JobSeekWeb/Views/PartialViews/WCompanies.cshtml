@{ 
    Layout = "";
}
<div class="row">
    <div class="col s12 m4" ng-repeat="item in list">
        <div class="card">
            <div class="card-image">
                <img src="~/Content/Moralde/Images/eriri.png" style="width:290px;" />
            </div>
            <div class="card-content">
                <span class="card-title">Card Title</span>
                <p>I am a very simple card. I am good at containing small bits of information. I am convenient because I require little markup to use effectively.</p>
            </div>
            <div class="card-action">
                <div class="row">
                    <div class="col s12 m6 center-btn">
                        <a class="waves-effect waves-light pink btn custom-width">Apply</a>
                    </div>
                    <div class="col s12 m6 center-btn">
                        <a class="waves-effect waves-light pink btn custom-width">Message</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>